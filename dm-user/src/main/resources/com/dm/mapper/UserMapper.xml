<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dm.study.cloud.dao.UserMapper">
    <select id="selectUserList" resultType="com.dm.study.cloud.po.DmUser">
        select * from dm.dm_user
        <where>
            <if test="p.email != null and p.email != ''">
                and email = #{p.email}
            </if>
        </where>
    </select>
</mapper>