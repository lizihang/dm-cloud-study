<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dm.study.cloud.dao.SysDictMapper">
    <select id="selectDictDetailList" resultType="sysDictDetail">
        select *
        from dm.dm_sys_dict_detail
        where dict_id = #{dictId}
    </select>
</mapper>